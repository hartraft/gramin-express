{
  "name": "gramin-express",
  "productName": "Gramin Express",
  "description": "Sync Strava activities from any Garmin device",
  "homepage": "http://salomvary.github.io/gramin-express",
  "author": {
    "name": "Márton Salomváry",
    "email": "salomvary@gmail.com"
  },
  "private": true,
  "version": "0.0.4-pre.1",
  "main": "app/main.js",
